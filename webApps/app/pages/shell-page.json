{
  "description": "",
  "routerFlow": "opty",
  "variables": {
    "messagesADP": {
      "type": "vb/ArrayDataProvider2",
      "defaultValue": {
        "keyAttributes": "id",
        "itemType": "page:messageType"
      }
    }
  },
  "metadata": {},
  "types": {
    "messageType": {
      "id": "string",
      "severity": "string",
      "category": "string",
      "summary": "string",
      "detail": "string",
      "timestamp": "string",
      "autoTimeout": "number"
    }
  },
  "eventListeners": {
    "application:navigateToItem": {
      "chains": [
        {
          "chainId": "navigationHandler",
          "parameters": {
            "item": "{{ $event.item }}",
            "updatedFrom": "{{ $event.updatedFrom }}"
          }
        }
      ]
    },
    "application:toggleDrawer": {
      "chains": [
        {
          "chainId": "toggleDrawer",
          "parameters": {}
        }
      ]
    },
    "vbNotification": {
      "chains": [
        {
          "chainId": "showNotification",
          "parameters": {
            "eventPayload": "{{ { id: $event.key, summary: $event.summary, detail: $event.message, severity: $event.type, autoTimeout: $event.displayMode === 'transient' ? 0 : -1 } }}"
          }
        }
      ]
    },
    "onMessageClose": {
      "chains": [
        {
          "chainId": "closeNotificationHandler",
          "parameters": {
            "eventMessage": "{{ $event.detail.message }}"
          }
        }
      ]
    }

  },
  "imports": {
    "components": {
      "oj-button": {
        "path": "ojs/ojbutton"
      },
      "oj-messages": {
        "path": "ojs/ojmessages"
      },
      "oj-drawer-layout": {
        "path": "ojs/ojdrawerlayout"
      }
    },
    "css": [
      "/resources/css/shell.css"
    ]
  },
  "title": "Redwood Support Agent App"
}